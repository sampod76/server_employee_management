{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 13,
    "sourceType": "module",
  },
  "plugins": ["@typescript-eslint"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier",
  ],
  "rules": {
    "@typescript-eslint/no-unused-vars": "warn",
    "@typescript-eslint/consistent-type-definitions": ["error", "type"],
    "no-undef": "error",
    "@typescript-eslint/ban-ts-comment": "off", // Disable the rule or set to 1 (warn) or 2 (error) if needed
    // "no-unreachable":"error",
    // "no-console":"error"
  },
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
  },
  "globals": {
    "process": "readonly",
  },
}
